[user]
[core]
    autocrlf = input
    safecrlf = true
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph
    type = cat-file -t
    dump = cat-file -p
[color]
    ui = auto

[diff "localizablestrings"]
    textconv = "iconv -f utf-16 -t utf-8"
